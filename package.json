{
  "private": true,
  "name": "sugarcube-website",
  "version": "0.9.0",
  "license": "GPL-3.0",
  "author": "Christo <christo@cryptodrunks.net>",
  "scripts": {
    "clean": "rimraf out .next",
    "compile": "next build && next export",
    "fix": "run-p --aggregate-output --continue-on-error lint:*:fix",
    "lint": "run-p -l --aggregate-output --continue-on-error 'lint:!(fix)'",
    "lint:css": "stylelint --cache --allow-empty-input **/*.css",
    "lint:css:fix": "yarn lint:css --fix",
    "lint:js": "eslint --ext js,ts,jsx,tsx --cache .",
    "lint:js:fix": "yarn lint:js --fix",
    "lint:json": "prettier --list-different --parser json **/*.json",
    "lint:json:fix": "prettier --parser json --write **/*.json",
    "lint:package.json": "prettier-package-json --list-different",
    "lint:package.json:fix": "prettier-package-json --write",
    "lint:ts": "tsc --noEmit",
    "prepare": "husky install",
    "start": "yarn watch",
    "watch": "run-p -l --continue-on-error watch:*",
    "watch:next": "next dev -H 0.0.0.0",
    "watch:tsc": "yarn lint:ts -w"
  },
  "dependencies": {
    "@headlessui/react": "^1.1.1",
    "@heroicons/react": "^1.0.1",
    "@mdx-js/react": "^1.6.22",
    "@sindresorhus/slugify": "^2.0.0",
    "clsx": "^1.1.1",
    "fp-ts": "^2.10.5",
    "gray-matter": "^4.0.3",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-optipng": "^8.0.0",
    "io-ts": "^2.2.16",
    "io-ts-reporters": "^1.2.2",
    "next": "^11.1.3",
    "next-compose-plugins": "^2.2.1",
    "next-mdx-remote": "^3.0.1",
    "next-optimized-images": "^2.6.2",
    "next-plugin-preact": "^3.0.4",
    "next-svgr": "^0.0.2",
    "preact": "^10.5.13",
    "preact-render-to-string": "^5.1.19",
    "prism-react-renderer": "^1.2.1",
    "react": "npm:@preact/compat@^0.0.4",
    "react-dom": "npm:@preact/compat@^0.0.4",
    "react-ssr-prepass": "npm:preact-ssr-prepass@^1.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@mdx-js/loader": "^1.6.22",
    "@svgr/webpack": "^5.5.0",
    "@tailwindcss/typography": "^0.4.1",
    "@types/mdx-js__react": "^1.5.3",
    "@types/react": "^17.0.5",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "autoprefixer": "^10.2.5",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.2.2",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unicorn": "^31.0.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "next-videos": "^1.4.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.14",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^14.0.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "prettier-package-json": "^2.5.0",
    "responsive-loader": "^2.3.0",
    "rimraf": "^3.0.2",
    "sharp": "^0.28.1",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "tailwindcss": "^2.1.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "typescript-language-server": "^0.5.1",
    "webp-loader": "^0.6.0"
  },
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ],
  "lint-staged": {
    "*.{json,rc}": "prettier --parser json --write",
    "package.json": "prettier-package-json --write",
    "*.{js,jsx,ts,tsx}": "eslint --ext js,ts,jsx,tsx --cache --fix",
    "*.css": "stylelint --cache --allow-empty-input"
  }
}
